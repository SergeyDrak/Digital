{
  "name": "sergeydrak/subscription-bundle",
  "type": "symfony-bundle",
  "description": "Symfony 5 test project for digital",
  "license": "MIT",
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Sergeydrak\\SubscriptionBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sergeydrak\\SubscriptionBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test" : "./vendor/bin/simple-phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.0.*"
    }
  }
}